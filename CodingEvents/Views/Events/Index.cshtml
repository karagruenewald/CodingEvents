@{
}
@*<link rel="stylesheet" type="text/css" href="~/css/table.css">*@
<h2>Events Lists</h2>
<a style ="text-decoration: underline" class="nav-link text-dark" asp-area="" asp-controller="Events" asp-action="Add">Add Event</a>

<table class="table table-striped">
    <tr>
        <th>Event</th>
        <th>Description</th>
    </tr>

    @foreach (KeyValuePair<string, string> item in ViewBag.events)
    {
        <tr>
            <td>@item.Key</td>
            <td>@item.Value</td>

        </tr>
    }

</table>