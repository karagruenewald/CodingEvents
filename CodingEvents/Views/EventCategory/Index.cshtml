@{
}
@using CodingEvents.Models
@model List<EventCategory>


<h1>Event Categories</h1>

<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>

    </tr>

    @foreach (var evt in Model)
    {
        <tr>
            <td>evt.Id</td>
            <td>evt.Name</td>

        </tr>

    }
</table>

